<head>

    <link rel="stylesheet" href="\css\signup.css">

</head>
<body>
  <form action="/signup" method="POST" class="formulario">
    <h1 class="formulario__titulo">Registrate</h1>

    <input type="text" class="formulario__input" name="usuario">
    <label for="" class="formulario__label">Nombre de usuario</label>

    <input type="email" class="formulario__input" name="email">
    <label for="" class="formulario__label">Correo electrónico</label>

    <input type="password" class="formulario__input" name="contraseña">
    <label for="" class="formulario__label">Contraseña</label>

    <input type="password" class="formulario__input" name="confirmacion">
    <label for="" class="formulario__label">Confirmar contraseña</label>

    <input type="text" class="formulario__input" name="telefono">
    <label for="" class="formulario__label">Número de teléfono</label>


    <input type="submit" class="formulario__submit">
  </form>

    <script src="\scripts\contacto.js"></script>

</body>